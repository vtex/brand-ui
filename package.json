{
  "name": "@vtex/brand-ui",
  "version": "0.0.0",
  "description": "VTEX brand component library",
  "author": "vtex",
  "license": "MIT",
  "repository": "vtex/brand-ui",
  "main": "dist/index.js",
  "module": "dist/brand-ui.esm.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "eslint src/ --ext .tsx,.ts",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release:patch": "releasy patch --stable --notes",
    "release:minor": "releasy minor --stable --notes",
    "release:major": "releasy major --stable --notes"
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^6.0.1",
    "@storybook/addon-knobs": "^6.0.1",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storysource": "^6.0.1",
    "@storybook/addon-viewport": "^6.0.1",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^5.3.19",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@vtex/prettier-config": "^0.3.1",
    "@vtex/tsconfig": "^0.5.0",
    "eslint-config-vtex-react": "^6.7.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "releasy": "^1.10.5",
    "ts-loader": "^8.0.1",
    "tsdx": "^0.13.2",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
